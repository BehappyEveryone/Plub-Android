<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>


    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="12dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="72dp"
            android:background="@drawable/bg_rectangle_filled_white_radius_10_border_f2f3f4"
            android:padding="16dp"
            android:paddingBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/image_view_profile"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/bg_circle_filled_e4e4e4"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/text_view_nick_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="16dp"
                android:textColor="@color/color_363636"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/button_kick_out"
                app:layout_constraintStart_toEndOf="@id/image_view_profile"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="닉네임닉네임닉네임" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button_kick_out"
                android:layout_width="56dp"
                android:layout_height="30dp"
                android:background="@drawable/bg_rectangle_filled_f2f3f4_radius_8"
                android:stateListAnimator="@null"
                android:text="@string/word_kick_out"
                android:textColor="@color/color_8c8c8c"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>